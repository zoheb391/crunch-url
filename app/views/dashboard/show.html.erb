<div class = "col-xs-10 col-xs-offset-2">
  <table>
    <caption><center><h3>My URLs</h3></center></caption>
      <tr>
          <td><b>Target URL</b></td>
          <td><b>Shortcut</b></td>
          <td><b>Delete</b></td>
      </tr>
    <% if @my_urls.present? %>
      <% @my_urls.each do |my_url| %>
          <tr>
            <td><%= my_url.target %></td>
            <td><a href = "crunch-url.herokuapp.com/<%= my_url.shortcut %>"><%= "crunch.url.herokuapp.com/#{my_url.shortcut}" %></a> </td>
            <td><%= link_to "DELETE", url_path(my_url), :method => :delete, :data =>{ :confirm => 'Are You Sure You Want To Delete This?'}, :class => 'btn btn-danger' %> </td>
          </tr>
      <% end %>
    <% end %>
  </table>
</div>